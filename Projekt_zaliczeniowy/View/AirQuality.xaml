<UserControl x:Class="Projekt_zaliczeniowy.View.AirQuality"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d"
         d:Height="450" d:Width="800">

        <Grid>
        
            <Grid.ColumnDefinitions>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
            <ColumnDefinition Width="*"></ColumnDefinition>
        </Grid.ColumnDefinitions>
        <Grid.RowDefinitions>
            <RowDefinition Height="160"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>
            <RowDefinition Height="50"></RowDefinition>

        </Grid.RowDefinitions>
        
        <StackPanel  Grid.Row="0" Grid.Column="2">
            <TextBlock  TextAlignment="Center" Background="Bisque" FontSize="20" Text="Id Sensor" ></TextBlock>
            <TextBlock  TextAlignment="Center" Background="AntiqueWhite" FontSize="20" Margin="0 5 0 0"  x:Name="Id_Air" ></TextBlock>
        </StackPanel>
        <StackPanel  Grid.ColumnSpan="2">
            
            <TextBlock Grid.Row="1" Grid.Column="0" Background="Bisque" FontSize="20" Margin="5 5 150 0"  Text="StCalcDate"></TextBlock>
            <TextBlock Grid.Row="2"  Background="AntiqueWhite" FontSize="20" Margin="5 5 150 0" x:Name="StCalcDate" ></TextBlock>
        </StackPanel>


        <TextBlock Background="Bisque"  Grid.Row="1" Grid.Column="0" TextAlignment="Center" FontSize="20" Margin="2 2 2 2" Text="so2IndexLevel"></TextBlock>
        <TextBlock  Grid.Row="2" Grid.Column="0" TextAlignment="Center" FontSize="20" Margin="2 2 2 2" x:Name="so2" ></TextBlock>

        <TextBlock  Background="Bisque" Grid.Row="1" Grid.Column="1" TextAlignment="Center" FontSize="20" Margin="2 2 2 2" Text="no2IndexLevel"></TextBlock>
        <TextBlock Grid.Row="2" Grid.Column="1" TextAlignment="Center" FontSize="20" Margin="2 2 2 2" x:Name="no2" ></TextBlock>

        <TextBlock  Background="Bisque"  Grid.Row="1" Grid.Column="2" TextAlignment="Center" FontSize="20" Margin="2 2 2 2" Text="pm10IndexLevel"></TextBlock>
        <TextBlock Grid.Row="2" Grid.Column="2" TextAlignment="Center" FontSize="20" Margin="2 2 2 2" x:Name="pm10" ></TextBlock>

        <TextBlock Background="Bisque" Grid.Row="1" Grid.Column="3" TextAlignment="Center" FontSize="20" Margin="2 2 2 2" Text="pm25IndexLevel"></TextBlock>
        <TextBlock Grid.Row="2" Grid.Column="3" TextAlignment="Center" FontSize="20" Margin="2 2 2 2" x:Name="pm25" ></TextBlock>

        <TextBlock Background="Bisque"  Grid.Row="1" Grid.Column="4" TextAlignment="Center" FontSize="20" Margin="2 2 2 2" Text="o3IndexLevel"></TextBlock>
        <TextBlock Grid.Row="2" Grid.Column="4" TextAlignment="Center" FontSize="20" Margin="2 2 2 2" x:Name="o3" ></TextBlock>
       
    </Grid>
           
 

</UserControl>
